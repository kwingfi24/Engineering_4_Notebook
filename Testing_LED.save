import RPi.GPIO as GPIO
GPIO.setmode(GPIO.BCM) #setting the numbering scheme as the BCM scheme
import time #importing time to use sleep command
GPIO.setwarnings(False)
GPIO.setup(21,GPIO.OUT) #Setting the red light as an output
GPIO.setup(26,GPIO.OUT) #Setting the yellow light as an output


GPIO.output(26,GPIO.LOW)
GPIO.output(21,GPIO.HIGH)
print ("Red on, Yellow off")
time.sleep(1)
GPIO.output(21,GPIO.LOW)
GPIO.output(26,GPIO.HIGH)
print ("Red off, Yellow on")
