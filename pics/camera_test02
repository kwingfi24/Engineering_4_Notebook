# Camera test 02 - 5 pictures with different effects
# Written by Kaitlyn Wingfield
# 1.19.22

import time
import picamera
effects = ['none', ]

with picamera.PiCamera() as camera:
    print ('running')
    camera.resolution = (1024, 768)
    camera.start_preview()
    # Camera warm-up time
    time.sleep(2)
      for s in effects:
      print("Effect: " + s)
      camera.image_effect = s
      camera.capture('picture' + s + '.jpg')

effects = ['none', 'negative', 'sketch', 'denoise', 'emboss', 'oilpaint', 'hatch', 'gpen', 'pastel', 'watercolor', 'film', 'blur', 'saturation']

print ('done')
